    "//": "--- [1] ENTORNO DE DESARROLLO ---",
    "dev": "pnpm nx serve store",
    "build": "pnpm nx build store",
    "start": "pnpm nx start store",
    "graph": "pnpm nx graph",

    "//--": "--- [2] CALIDAD, VALIDACIÓN Y LINTING ---",
    "lint": "pnpm nx run-many --target=lint --all --fix",
    "format:write": "prettier --write . --log-level warn",
    "format:check": "prettier --check . --log-level warn",
    "check-types": "tsc --noEmit",
    "validate": "pnpm gen:all && pnpm format:write && pnpm lint && pnpm check-types",

    "//---": "--- [3] ORQUESTADORES DE PRUEBAS SOBERANAS ---",
    "test": "pnpm nx run-many --target=test --all --passWithNoTests",
    "test:watch": "pnpm nx run-many --target=test --all --watch --passWithNoTests",
    "test:affected": "pnpm nx affected --target=test --passWithNoTests",

    "//----": "--- [4] GENERACIÓN DE CÓDIGO Y ARTEFACTOS ---",
    "gen:types": "pnpm supabase:gen-types",
    "gen:icons": "tsx --tsconfig tsconfig.scripts.json scripts/generation/generate-lucide-icon-enum.ts",
    "gen:all": "pnpm gen:types && pnpm gen:icons",

    "//-----": "--- [5] GESTIÓN DE SUPABASE (CLI) ---",
    "supabase:login": "supabase login",
    "supabase:link": "supabase link --project-ref lbdhtkfsnosfttorlblh",
    "supabase:gen-types": "tsx --tsconfig tsconfig.scripts.json scripts/supabase/generation/generate-db-types.ts",
    "supabase:db:push": "supabase db push",

    "//------": "--- [6] SEEDING DE BASE DE DATOS ---",
    "db:seed:superuser": "tsx --tsconfig tsconfig.scripts.json scripts/run-with-env.ts scripts/supabase/seeding/superuser.ts",
    "db:seed:all": "pnpm db:seed:superuser",

    "//-------": "--- [7] DIAGNÓSTICOS Y SCRIPTS DE SISTEMA ---",
    "diag:auth:contracts": "tsx --tsconfig tsconfig.scripts.json scripts/diagnostics/auth-contracts.diag.ts",
    "diag:supabase:connect": "tsx --tsconfig tsconfig.scripts.json scripts/run-with-env.ts scripts/supabase/connect.ts"
  },
